{
    "compilerOptions": {
        "target": "es5",
        "lib": ["DOM","DOM.Iterable","ESNext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "jsx": "preserve",
        "incremental": true,
        "baseUrl": ".",

    },
    "include": ["src"],
    "exclude": ["node_modules"]
}